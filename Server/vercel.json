{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "dist/index.js": {
      "runtime": "nodejs18.x"
    }
  }
}