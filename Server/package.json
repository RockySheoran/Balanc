    "@types/redis": "^4.0.10",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "ejs": "^3.1.10",
    "nodemon": "^3.1.9",
    "prisma": "^6.5.0",
    "rimraf": "^6.0.1",
